		"builtin-modules": "5.0.0",
		"esbuild": "0.27.2",
		"eslint": "^9.39.2",
		"obsidian": "latest",
		"tslib": "2.8.1",
		"typescript": "5.9.3",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
